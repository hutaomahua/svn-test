package com.lyht.business.pub.enums;
import com.lyht.base.common.enums.ExceptionEnums;

/**
 * 	lyht系统异常枚举
 * 
 * @author hxl
 *
 */
public enum PubExceptionEnums implements ExceptionEnums {
	FILE_UPLOAD_FAILURE("-101", "上传失败");

	private String code;
	private String msg;

	PubExceptionEnums(String code, String msg) {
		this.code = code;
		this.msg = msg;
	}

	@Override
	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getMsg() {
		return msg;
	}

	public void setMsg(String msg) {
		this.msg = msg;
	}

}
