@Entity
@Table(name = "v_payment_confirmation")
@ApiModel(description = "已完成兑付申请的协议")
public class VPaymentConfirmation {

	@ApiModelProperty(value = "id")
    @Id
    @Column(name = "id" , nullable = false )
	private String id;

	@ApiModelProperty(value = "nm")
	@Column(name = "nm")
	private String nm;

	@ApiModelProperty(value = "权属人内码")
	@Column(name = "owner_nm")
	private String ownerNm;

	@ApiModelProperty(value = "协议编号")
	@Column(name = "protocol_code")
	private String protocolCode;

	@ApiModelProperty(value = "协议类型 1资金代管协议 0补偿协议")
	@Column(name = "protocol_type")
	private String protocolType;

	@ApiModelProperty(value = "申请发起时间")
	@Column(name = "apply_time")
	private String applyTime;

	@ApiModelProperty(value = "申请批次")
	@Column(name = "apply_batch")
	private String applyBatch;

	@ApiModelProperty(value = "申请金额")
	@Column(name = "apply_amount")
	private String applyAmount;

	@ApiModelProperty(value = "申请发起人")
	@Column(name = "proposer")
	private String proposer;

	@ApiModelProperty(value = "申请人单位")
	@Column(name = "proposer_dept")
	private String proposerDept;

	@ApiModelProperty(value = "收款人")
	@Column(name = "payee")
	private String payee;

	@ApiModelProperty(value = "支付方式")
	@Column(name = "payment_method")
	private String paymentMethod;

	@ApiModelProperty(value = "银行卡号")
	@Column(name = "bank_card")
	private String bankCard;

	@ApiModelProperty(value = "开户行")
	@Column(name = "deposit_bank")
	private String depositBank;

	@ApiModelProperty(value = "申请描述")
	@Column(name = "description")
	private String description;

	@ApiModelProperty(value = "备注")
	@Column(name = "remark")
	private String remark;

	@ApiModelProperty(value = "兑付申请流程实例id")
	@Column(name = "applied_process_id")
	private String appliedProcessId;

	@ApiModelProperty(value = "申请流程状态")
	@Column(name = "applied_status")
	private String appliedStatus;

	@ApiModelProperty(value = "申请流程状态明文")
	@Column(name = "applied_status_cn")
	private String appliedStatusCn;

	@ApiModelProperty(value = "兑付确认批次号")
	@Column(name = "confirmation_batch")
	private String confirmationBatch;

	@ApiModelProperty(value = "兑付确认批次审核流程实例ID")
	@Column(name = "confirmation_process_id")
	private String confirmationProcessId;

	@ApiModelProperty(value = "确认流程状态")
	@Column(name = "confirmation_status")
	private String confirmationStatus;

	@ApiModelProperty(value = "确认流程状态明文")
	@Column(name = "confirmation_status_cn")
	private String confirmationStatusCn;

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getNm() {
		return nm;
	}

	public void setNm(String nm) {
		this.nm = nm;
	}

	public String getOwnerNm() {
		return ownerNm;
	}

	public void setOwnerNm(String ownerNm) {
		this.ownerNm = ownerNm;
	}

	public String getProtocolCode() {
		return protocolCode;
	}

	public void setProtocolCode(String protocolCode) {
		this.protocolCode = protocolCode;
	}

	public String getProtocolType() {
		return protocolType;
	}

	public void setProtocolType(String protocolType) {
		this.protocolType = protocolType;
	}

	public String getApplyTime() {
		return applyTime;
	}

	public void setApplyTime(String applyTime) {
		this.applyTime = applyTime;
	}

	public String getApplyBatch() {
		return applyBatch;
	}

	public void setApplyBatch(String applyBatch) {
		this.applyBatch = applyBatch;
	}

	public String getApplyAmount() {
		return applyAmount;
	}

	public void setApplyAmount(String applyAmount) {
		this.applyAmount = applyAmount;
	}

	public String getProposer() {
		return proposer;
	}

	public void setProposer(String proposer) {
		this.proposer = proposer;
	}

	public String getProposerDept() {
		return proposerDept;
	}

	public void setProposerDept(String proposerDept) {
		this.proposerDept = proposerDept;
	}

	public String getPayee() {
		return payee;
	}

	public void setPayee(String payee) {
		this.payee = payee;
	}

	public String getPaymentMethod() {
		return paymentMethod;
	}

	public void setPaymentMethod(String paymentMethod) {
		this.paymentMethod = paymentMethod;
	}

	public String getBankCard() {
		return bankCard;
	}

	public void setBankCard(String bankCard) {
		this.bankCard = bankCard;
	}

	public String getDepositBank() {
		return depositBank;
	}

	public void setDepositBank(String depositBank) {
		this.depositBank = depositBank;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public String getAppliedProcessId() {
		return appliedProcessId;
	}

	public void setAppliedProcessId(String appliedProcessId) {
		this.appliedProcessId = appliedProcessId;
	}

	public String getAppliedStatus() {
		return appliedStatus;
	}

	public void setAppliedStatus(String appliedStatus) {
		this.appliedStatus = appliedStatus;
	}

	public String getAppliedStatusCn() {
		return appliedStatusCn;
	}

	public void setAppliedStatusCn(String appliedStatusCn) {
		this.appliedStatusCn = appliedStatusCn;
	}

	public String getConfirmationBatch() {
		return confirmationBatch;
	}

	public void setConfirmationBatch(String confirmationBatch) {
		this.confirmationBatch = confirmationBatch;
	}

	public String getConfirmationProcessId() {
		return confirmationProcessId;
	}

	public void setConfirmationProcessId(String confirmationProcessId) {
		this.confirmationProcessId = confirmationProcessId;
	}

	public String getConfirmationStatus() {
		return confirmationStatus;
	}

	public void setConfirmationStatus(String confirmationStatus) {
		this.confirmationStatus = confirmationStatus;
	}

	public String getConfirmationStatusCn() {
		return confirmationStatusCn;
	}

	public void setConfirmationStatusCn(String confirmationStatusCn) {
		this.confirmationStatusCn = confirmationStatusCn;
	}

}
