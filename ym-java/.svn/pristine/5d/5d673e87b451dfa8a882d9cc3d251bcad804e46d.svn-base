package com.lyht.business.abm.household.vo;

import io.swagger.annotations.ApiModelProperty;

import java.util.List;

/**
 * @Description:
 * @Author: xzp
 * @Date: 2020/9/1 9:19
 **/
public class MergeParamVO {

    @ApiModelProperty("权属人内码")
    private String ownerNm;

    @ApiModelProperty("户主集合")
    private List<MergeOwnerKeyValueVO> list;

    @ApiModelProperty("家庭指标集合")
    private List<MergeOwnerFamilyKeyValueVO> familyList;

    public String getOwnerNm() {
        return ownerNm;
    }

    public void setOwnerNm(String ownerNm) {
        this.ownerNm = ownerNm;
    }

    public List<MergeOwnerKeyValueVO> getList() {
        return list;
    }

    public void setList(List<MergeOwnerKeyValueVO> list) {
        this.list = list;
    }

    public List<MergeOwnerFamilyKeyValueVO> getFamilyList() {
        return familyList;
    }

    public void setFamilyList(List<MergeOwnerFamilyKeyValueVO> familyList) {
        this.familyList = familyList;
    }
}
